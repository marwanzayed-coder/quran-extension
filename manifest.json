{
  "name": "Quran Extension",
  "description": "The Holy Quran website - an Islamic website that helps people to read the Holy Quran",
  "version": "1.2",
  "manifest_version": 3,
  "background": {
    "service_worker": "/sw.js"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["tabs"],
  "action": {
    "default_icon": "/images/icon-128.png",
    "default_title": "Quran Extension"
  },
  "icons": {
    "128": "/images/icon-128.png"
  }
}
